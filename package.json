  "license": "MIT",
  "repository": "git+https://github.com/koenoe/react-virtualized-grid.git",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.prod.js",
    "build:open": "yarn build && http-server ./dist -p 4000 -o",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "flow": "flow check",
    "flow:cover": "flow-coverage-report -i 'src/**/*.js'",
    "flow:watch": "flow-bro watch",
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "yarn lint --watch",
    "prebuild": "yarn lint && yarn test && yarn clean-dist",
    "serve": "webpack-serve --port 3000 --log-level error --config ./webpack.config.dev.js",
    "start": "concurrently -k -r -s first \"yarn test:watch\" \"yarn serve\" \"yarn lint:watch\"",
    "test": "jest",
    "test:cover": "yarn test --coverage",
    "test:coveralls": "yarn test:cover && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watch"
  },
  "dependencies": {
